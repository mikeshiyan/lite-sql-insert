{
    "name": "shiyan/lite-sql-insert",
    "description": "Lightweight SQL insertion service.",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "ext-PDO": "^7.1"
    },
    "require-dev": {
        "ext-pdo_sqlite": "^7.1",
        "phpunit/phpunit": "^6.4",
        "phpunit/dbunit": "^3.0"
    },
    "suggest": {
        "shiyan/iterate": "Allows inserting in iteration"
    },
    "autoload": {
        "psr-4": {
            "Shiyan\\LiteSqlInsert\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shiyan\\LiteSqlInsert\\tests\\": "tests/"
        }
    },
    "keywords": ["lite", "sql", "insert", "php", "pdo", "oop"],
    "support": {
        "source": "https://github.com/mikeshiyan/lite-sql-insert",
        "issues": "https://github.com/mikeshiyan/lite-sql-insert/issues"
    },
    "authors": [
        {
            "name": "Mike Shiyan",
            "homepage": "https://github.com/mikeshiyan"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit --coverage-text --whitelist src tests"
    }
}
